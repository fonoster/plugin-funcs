{
  "name": "@fonos/plugin-funcs",
  "description": "Managament for Fonos Functions",
  "version": "0.1.0",
  "author": "Wardner Lara @wardner, Raul Sanchez @speedymonster",
  "scripts": {
    "postpack": "rimraf oclif.manifest.json",
    "prepack": "rimraf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "bugs": "https://github.com/fonoster/fonos-plugin-funcs/issues",
  "dependencies": {
    "@fonos/funcs": "^0.0.97",
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@types/chai-as-promised": "^7.1.4",
    "axios": "^0.21.1",
    "cli-ux": "^5.5.1",
    "easy-table": "^1.1.1",
    "fs-extra": "^10.0.0",
    "inquirer": "^8.0.0",
    "ncp": "^2.0.0",
    "node-plop": "^0.26.2",
    "plop": "^2.7.4",
    "prettyjson": "^1.2.1",
    "tslib": "^1.14.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.0",
    "@oclif/plugin-help": "^3.2.2",
    "@oclif/test": "^1.2.8",
    "@types/chai": "^4.2.17",
    "@types/mocha": "^5.2.7",
    "@types/prettyjson": "0.0.29",
    "@types/sinon-chai": "^3.2.5",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "dotenv": "^8.2.0",
    "globby": "^10.0.2",
    "mocha": "^5.2.0",
    "nyc": "^14.1.1",
    "rimraf": "^3.0.2",
    "sinon": "^10.0.0",
    "sinon-chai": "^3.6.0",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.9"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/Wardner/func",
  "keywords": [
    "oclif-plugin"
  ],
  "license": "MIT",
  "oclif": {
    "commands": "./lib/commands",
    "topics": {
      "funcs": {
        "description": "Fonos command"
      }
    },
    "bin": "oclif-example",
    "devPlugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "https://github.com/fonoster/fonos-plugin-funcs"
}
